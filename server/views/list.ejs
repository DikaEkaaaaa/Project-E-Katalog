<body class="p-6 bg-gray-900">
  <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 xl:grid-cols-3 gap-4">
    <% for(let i=0;i<data.length;i++) { %>
      <div class="w-full">

        <div class="flex font-sans h-48 shadow-xl bg-white rounded">
          <div class="flex-none w-28 relative">
            <img src="https://tailwindcss.com/_next/static/media/classic-utility-jacket.82031370.jpg" alt=""
              class="rounded-l absolute inset-0 w-full h-full object-cover" loading="lazy" />
          </div>
          <form class="flex-auto p-6">
            <div class="flex flex-col justify-between h-full">
              <div class="flex flex-wrap">
                <h1 class="flex-auto text-lg font-semibold text-slate-900">
                  <%- data[i].nama %>
                </h1>
                <div class="w-full flex-none text-sm font-medium text-slate-700 mt-2">
                  <%- data[i].id %>
                </div>
              </div>

              <p class="text-sm text-slate-700">
                <%- data[i].quote %>
              </p>
            </div>
          </form>
        </div>
      </div>
      <% } %>
  </div>

